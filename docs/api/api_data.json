[  {    "type": "post",    "url": "/",    "title": "Demonstrates how to handle JSON input data.",    "name": "EchoRequest",    "group": "ExampleEndpoints",    "version": "0.0.1",    "examples": [      {        "title": "Example usage:",        "content": "curl http://localhost:8080/api/v1 -X POST -H \"Content-Type: application/json\" -d '{\"TM\":\"19790905\", \"mk\":\"19790923\"}'",        "type": "curl"      }    ],    "success": {      "fields": {        "200": [          {            "group": "200",            "type": "Object",            "optional": false,            "field": "echoResponse",            "description": "<p>A result object with the data from the input body.</p>"          }        ]      },      "examples": [        {          "title": "Example response:",          "content": "HTTP/1.1 200 OK\nContent-type: application/json; charset=utf-8\n\n{\n  \"data\": {\n    \"echo\": {\n      \"TM\": \"19790905\",\n      \"mk\": \"19790923\"\n    }\n  },\n  \"errors\": [],\n  \"success\": true\n}",          "type": "json"        }      ]    },    "filename": "src/api/v1/root.ts",    "groupTitle": "ExampleEndpoints"  },  {    "type": "get",    "url": "/",    "title": "Gets information about the host.",    "name": "GetHostInfo",    "group": "ExampleEndpoints",    "version": "0.0.1",    "examples": [      {        "title": "Example usage:",        "content": "curl http://localhost:8080/api/v1",        "type": "curl"      }    ],    "success": {      "fields": {        "200": [          {            "group": "200",            "type": "Object",            "optional": false,            "field": "hostInfo",            "description": "<p>Host information.</p>"          }        ]      },      "examples": [        {          "title": "Example response:",          "content": "HTTP/1.1 200 OK\nContent-type: application/json; charset=utf-8\n\n{\n  \"data\": {\n    \"cpu_load\": 0.07503075030750306,\n    \"